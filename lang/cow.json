{
    "terminals":["module","preamble","postamble","depends","namespace",
        "bindings","postamble","binds","imports","{","}","(",")","to","value","text",",",
        "rebase","assert","goto","scope","set","panic","call","layout","bind","lock","create",
        "curr"],
    "generics":{
        "name":"^[a-zA-Z_]+[a-zA-Z_0-9]*$",
        "number":"^[+-]?[0-9]+$",
        "raw_assembly":"^[][+<>^_-]+$",
        "bound_keyword":"^[a-zA-Z_]+[a-zA-Z0-9]+[a-zA-Z_0-9]*$"},
    "rules":{
        "module_closure":["name { preamble_closure depends_closure namespace_closure bindings_closure postamble_closure }"],
        "preamble_closure":["preamble { inline_text }",null],
        "depends_closure":["depends { module_names }"],
        "namespace_closure":["namespace { functional_blocks }"],
        "functional_blocks":["function_closure functional_blocks",
                            "nested_namespace_closure functional_blocks",
                            null],
        "nested_namespace_closure":["namespace name { functional_blocks }"],
        "import_chain":["imports module_names name",null],
        "binding_chain":["binds module_names name",null],
        "module_names":["module_names name",null],
        "function_closure":["name { inline_text }"],
        "bindings_closure":["bindings { binding_closures }",null],
        "binding_closures":["binding_block binding_closures",null],
        "binding_block":["name modifier_declaration { binding_text }"],
        "postamble_closure":["postamble { binding_text }",null],
        "binding_text":["keyword_closure binding_text","text_token binding_text",null],
        "modifier_declaration":["( modifier_list )",null],
        "modifier_list":["modifier_arg , modifier_list","modifier_arg"],
        "modifier_arg":["text name","layout name","value name"],
        "inline_text":["text_token inline_text","inline_closure inline_text",null],
        "text_token":["raw_assembly modifier","bound_keyword modifier"],
        "modifier":["( name modifier_chain )","( number modifier_chain )","( raw_assembly modifier_chain )",null],
        "modifier_chain":["name modifier_chain","number modifier_chain","raw_assembly modifier_chain",null],
        "inline_closure":["keyword_closure","call_closure"],
        "keyword_closure":[ "scope { inline_text }",
                            "lock { inline_text }",
                            "bind { bind_statement }",
                            "layout { layout_statement }",
                            "set { set_statement }",
                            "goto { goto_statement }",
                            "rebase { number }",
                            "create { data_address number }",
                            "assert { assert_statement }",
                            "value { value_statement }"
                        ],
        "value_statement":["name number","name number value_statement"],
        "call_closure":["call { call_path }"],
        "call_path":["name","name name"],
        "unimplemented":["panic"],
        "bind_statement":["name","name to non_empty_binding_text"],
        "non_empty_binding_text":["keyword_closure binding_text","text_token binding_text"],
        "layout_statement":["name number layout_statement","name number","name curr"],
        "set_statement":["set_pair set_statement","set_pair"],
        "set_pair":["data_target data_source"],
        "data_target":["name","data_address"],
        "data_source":["name","number","data_address"],
        "data_address":["{ number }"],
        "goto_statement":["name","number"],
        "assert_statement":["value data_address number"]
    
    },
    "start":"module_closure"
} 